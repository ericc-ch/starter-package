{
  "name": "pkg-placeholder",
  "version": "0.0.0",
  "description": "_description_",
  "keywords": [
    "pkg-placeholder"
  ],
  "homepage": "https://github.com/ericc-ch/pkg-placeholder",
  "bugs": "https://github.com/ericc-ch/pkg-placeholder/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ericc-ch/pkg-placeholder.git"
  },
  "author": {
    "name": "Erick Christian",
    "email": "erickchristian48@gmail.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    }
  },
  "bin": {
    "pkg-placeholder": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "format": "prettier --write .",
    "lint": "oxlint --type-aware",
    "prepare": "simple-git-hooks && ./scripts/release.ts",
    "prepack": "bun run build",
    "release": "bumpp && npm publish",
    "start": "bun src/cli.ts",
    "typecheck": "tsgo"
  },
  "simple-git-hooks": {
    "pre-commit": "bun run format",
    "pre-push": "bun run typecheck && bun test && bun run lint"
  },
  "dependencies": {
    "@effect/cli": "^0.73.1",
    "@effect/platform": "^0.94.2",
    "@effect/platform-bun": "^0.87.1",
    "effect": "^3.19.15",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@effect/language-service": "^0.72.0",
    "@types/bun": "^1.3.6",
    "@types/yargs": "^17.0.35",
    "@typescript/native-preview": "^7.0.0-dev.20260124.1",
    "bumpp": "^10.4.0",
    "oxlint": "^1.41.0",
    "oxlint-tsgolint": "^0.11.1",
    "prettier": "^3.8.1",
    "simple-git-hooks": "^2.13.1",
    "tsdown": "^0.20.1",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
